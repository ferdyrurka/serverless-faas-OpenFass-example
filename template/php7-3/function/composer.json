{
  "name": "function-rest-php7.2",
  "description": "Template for function in PHP 7.2",
  "type": "project",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.1.*",
    "mockery/mockery": "1.2.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "classmap-authoritative": true
  }
}